<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ac.dao.UserMapper">
    <resultMap id="BaseResultMap" type="com.ac.model.User">
        <id column="id" property="id" jdbcType="VARCHAR"/>
        <result column="name" property="name" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="user_result">
        ID,NAME
    </sql>

    <select id="queryUser" resultMap="BaseResultMap">
        SELECT <include refid="user_result"/> FROM test_user
    </select>


    <insert id="batchSave" parameterType="com.ac.model.User">
        INSERT INTO test_user(name) VALUES(#{name})
    </insert>

</mapper>
